---
import Article from "../../components/Article.astro";
import RootLayout from "../../layouts/RootLayout.astro";

const pageName = "Ramp";
const prevPage = {
  name: "Interaction",
  href: "/deckbuilding/interaction",
};

const nextPage = {
  name: "Influences",
  href: "/influences",
};
---

<RootLayout title={pageName}>
  <Article
    pageName={pageName}
    collectionName="deckbuilding-ramp"
    nextPage={nextPage}
    prevPage={prevPage}
  />
</RootLayout>
