---
import Article from "../../components/Article.astro";
import RootLayout from "../../layouts/RootLayout.astro";

const pageName = "Card Advantage";
const prevPage = {
  name: "Template",
  href: "/deckbuilding/template",
};

const nextPage = {
  name: "Interaction",
  href: "/deckbuilding/interaction",
};
---

<RootLayout title={pageName}>
  <Article
    pageName={pageName}
    collectionName="deckbuilding-card-advantage"
    nextPage={nextPage}
    prevPage={prevPage}
  />
</RootLayout>
