---
import RootLayout from "../../layouts/RootLayout.astro";
import Article from "../../components/Article.astro";

const pageName = "Threat Levels";

const prevPage = {
  name: "Tutors",
  href: "/influences/tutors",
};

const nextPage = {
  name: "Rule 0 Conversations",
  href: "/rule-zero",
};
---

<RootLayout title={pageName}>
  <Article
    pageName={pageName}
    collectionName="influences-threat-levels"
    nextPage={nextPage}
    prevPage={prevPage}
  />
</RootLayout>
