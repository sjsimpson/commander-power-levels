---
import Article from "../../components/Article.astro";
import RootLayout from "../../layouts/RootLayout.astro";

const pageName = "Influences";

const prevPage = {
  name: "Ramp",
  href: "/deckbuilding/ramp",
};

const nextPage = {
  name: "Strategies",
  href: "/influences/strategies",
};
---

<RootLayout title={pageName}>
  <Article
    pageName={pageName}
    collectionName="influences-overview"
    nextPage={nextPage}
    prevPage={prevPage}
  />
</RootLayout>
